@import '../../variables'
.numeric-dir
  display inline-block
  position relative
  .up, .down
    position absolute
    top 0
    right 0
    width 30px
    height 50%
    background #ccc
    display flex
    justify-content center
    no-select()
    cursor default
    box-shadow inset -1px -1px 2px #b7b7b7, inset 1px 1px 2px #e6e6e6
    line-height 2rem
  .down
    top 50%